<h1 class="text-center mb-4 mt-4"><%= @user.username %>'s profile</h1>

<%= gravatar_for @user, size: 200 %>   

<div class="text-center mt-4">
<% if logged_in? && @user == current_user %>
<%= link_to "Edit your profile", edit_user_path(@user), class: "btn btn-outline-secondary" %>
<% end %>
</div>

<h1 class="text-center mb-4 mt-4">Setups</h1>

<div class="flickr_pagination mb-4">
      <%= will_paginate @setups, :continer => false %>
</div>
<%= render 'setups/setup' %>

<div class="flickr_pagination mb-4">
      <%= will_paginate @setups, :continer => false %>
</div>